<%= form_tag searches_path do %>
  <div class="form_group">
    <%= label_tag :name %><br>
    <%= text_field_tag :name %>
  </div><br>
  <div class="form_group">
    <%= label_tag(:city, "City") %><br>
    <%= text_field_tag(:city, "Portland" ) %>
  </div><br>
  <%= submit_tag "Search", :class => "btn btn-primary" %>
<% end %>
